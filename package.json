{
  "name": "ai-agents",
  "version": "0.2.0",
  "description": "Specialized AI agents for software engineering tasks - from quick fixes to full app development",
  "type": "module",
  "main": "src/cli.ts",
  "bin": {
    "ai-agent": "src/cli.ts"
  },
  "scripts": {
    "dev": "bun run src/cli.ts",
    "bugfix": "bun run src/cli.ts bugfix",
    "feature": "bun run src/cli.ts feature",
    "refactor": "bun run src/cli.ts refactor",
    "build": "bun run src/cli.ts build",
    "resume": "bun run src/cli.ts resume",
    "status": "bun run src/cli.ts status",
    "typecheck": "bun x tsc --noEmit",
    "lint": "bun x eslint src/"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.61",
    "commander": "^14.0.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  }
}
